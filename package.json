{
	"name": "ora-cjs",
	"version": "0.0.2",
	"description": "Elegant terminal spinner (cjs version)",
	"license": "MIT",
	"repository": "sindresorhus/ora",
	"funding": "https://github.com/sponsors/sindresorhus",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "https://sindresorhus.com"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"test": "xo && ava && tsd",
		"build": "rimraf dist && tsup src/index.ts --dts --format cjs",
		"dev": "rimraf dist && tsup src/index.ts --dts --format cjs --watch"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"cli",
		"spinner",
		"spinners",
		"terminal",
		"term",
		"console",
		"ascii",
		"unicode",
		"loading",
		"indicator",
		"progress",
		"busy",
		"wait",
		"idle"
	],
	"dependencies": {
		"chalk": "^4.1.2",
		"cli-cursor": "^4.0.0",
		"cli-spinners": "^2.9.0",
		"is-interactive": "^2.0.0",
		"is-unicode-supported": "^1.3.0",
		"log-symbols": "^5.1.0",
		"stdin-discarder": "^0.1.0",
		"strip-ansi": "^7.0.1",
		"wcwidth": "^1.0.1"
	},
	"devDependencies": {
		"@types/node": "^20.1.4",
		"@types/wcwidth": "^1.0.0",
		"ava": "^5.2.0",
		"get-stream": "^6.0.1",
		"rimraf": "^5.0.1",
		"transform-tty": "^1.0.11",
		"tsd": "^0.28.1",
		"tsup": "^6.7.0",
		"typescript": "^5.1.3",
		"xo": "^0.54.2"
	},
	"xo": {
		"rules": {
			"@typescript-eslint/no-redundant-type-constituents": "off"
		}
	}
}
